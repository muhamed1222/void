{
  "name": "white-room",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:watch": "jest --watch --testPathIgnorePatterns=e2e",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=e2e",
    "e2e": "detox test -c ios.sim.debug",
    "e2e:android": "detox test -c android.emu.debug",
    "e2e:build": "detox build -c ios.sim.debug",
    "e2e:build:android": "detox build -c android.emu.debug"
  },
  "dependencies": {
    "expo": "~51.0.0",
    "react": "18.2.0",
    "react-native": "0.74.4",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "zustand": "^4.4.7",
    "react-native-mmkv": "^2.11.0",
    "expo-sqlite": "~14.0.3",
    "@shopify/flash-list": "1.6.4",
    "expo-notifications": "~0.28.11",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.0",
    "@testing-library/react-native": "^12.4.3",
    "detox": "^20.18.1"
  },
  "private": true
}